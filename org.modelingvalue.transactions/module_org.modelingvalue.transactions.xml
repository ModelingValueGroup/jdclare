<?xml version="1.0" encoding="UTF-8"?>
<project name="module_org.modelingvalue.transactions" default="compile.module.org.modelingvalue.transactions">
  <dirname property="module.org.modelingvalue.transactions.basedir" file="${ant.file.module_org.modelingvalue.transactions}"/>
  
  <property name="module.jdk.home.org.modelingvalue.transactions" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.org.modelingvalue.transactions" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.org.modelingvalue.transactions" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.org.modelingvalue.transactions" value="-encoding UTF-8 -source 9 -target 9 ${compiler.args}"/>
  
  <property name="org.modelingvalue.transactions.output.dir" value="${module.org.modelingvalue.transactions.basedir}/../out/production/org.modelingvalue.transactions"/>
  <property name="org.modelingvalue.transactions.testoutput.dir" value="${module.org.modelingvalue.transactions.basedir}/../out/test/org.modelingvalue.transactions"/>
  
  <path id="org.modelingvalue.transactions.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="org.modelingvalue.transactions.module.production.classpath">
    <path refid="${module.jdk.classpath.org.modelingvalue.transactions}"/>
    <pathelement location="${org.modelingvalue.collections.output.dir}"/>
  </path>
  
  <path id="org.modelingvalue.transactions.runtime.production.module.classpath">
    <pathelement location="${org.modelingvalue.transactions.output.dir}"/>
    <path refid="org.modelingvalue.collections.runtime.production.module.classpath"/>
  </path>
  
  <path id="org.modelingvalue.transactions.module.classpath">
    <path refid="${module.jdk.classpath.org.modelingvalue.transactions}"/>
    <pathelement location="${org.modelingvalue.transactions.output.dir}"/>
    <pathelement location="${org.modelingvalue.collections.testoutput.dir}"/>
    <pathelement location="${org.modelingvalue.collections.output.dir}"/>
  </path>
  
  <path id="org.modelingvalue.transactions.runtime.module.classpath">
    <pathelement location="${org.modelingvalue.transactions.testoutput.dir}"/>
    <pathelement location="${org.modelingvalue.transactions.output.dir}"/>
    <path refid="org.modelingvalue.collections.runtime.module.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.org.modelingvalue.transactions">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.org.modelingvalue.transactions">
    <patternset refid="excluded.from.module.org.modelingvalue.transactions"/>
  </patternset>
  
  <path id="org.modelingvalue.transactions.module.sourcepath">
    <dirset dir="${module.org.modelingvalue.transactions.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.org.modelingvalue.transactions" depends="compile.module.org.modelingvalue.transactions.production,compile.module.org.modelingvalue.transactions.tests" description="Compile module org.modelingvalue.transactions"/>
  
  <target name="compile.module.org.modelingvalue.transactions.production" depends="compile.module.org.modelingvalue.collections" description="Compile module org.modelingvalue.transactions; production classes">
    <mkdir dir="${org.modelingvalue.transactions.output.dir}"/>
    <javac destdir="${org.modelingvalue.transactions.output.dir}" debug="${compiler.debug}"
           nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true"
           executable="${module.jdk.bin.org.modelingvalue.transactions}/javac">
      <compilerarg line="${compiler.args.org.modelingvalue.transactions}"/>
      <bootclasspath refid="org.modelingvalue.transactions.module.bootclasspath"/>
      <classpath refid="org.modelingvalue.transactions.module.production.classpath"/>
      <src refid="org.modelingvalue.transactions.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.org.modelingvalue.transactions"/>
    </javac>
    
    <copy todir="${org.modelingvalue.transactions.output.dir}">
      <fileset dir="${module.org.modelingvalue.transactions.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.org.modelingvalue.transactions.tests" depends="compile.module.org.modelingvalue.transactions.production" description="compile module org.modelingvalue.transactions; test classes" unless="skip.tests"/>
  
  <target name="clean.module.org.modelingvalue.transactions" description="cleanup module">
    <delete dir="${org.modelingvalue.transactions.output.dir}"/>
    <delete dir="${org.modelingvalue.transactions.testoutput.dir}"/>
  </target>
</project>